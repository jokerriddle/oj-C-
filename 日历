#include<iostream>
using namespace std;
int main()
{
	int n,i;
while(scanf("%d",&n) != EOF)
{
	i=n%7;
	switch(i)
	{
		case 1:
			cout<<1<<endl;
			break;
			case 2:
			cout<<2<<endl;
			break;
			case 3:
			cout<<3<<endl;
			break;
			case 4:
			cout<<4<<endl;
			break;
			case 5:
			cout<<5<<endl;
			break;
			case 6:
			cout<<6<<endl;
			break;
			case 0:
			cout<<7<<endl;
			break;
	}
}
	return 0;
}
